<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:beans="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <beans:bean name="jsonView" class="org.springframework.web.servlet.view.json.MappingJackson2JsonView" p:contentType="text/html" />

    <beans:bean name="barcodeView" class="com.framework.innodale.controller.BarcodeView" p:contentType="text/html" />

    <beans:bean name="imageView" class="com.framework.innodale.controller.ImageView" p:contentType="text/html" />

    <beans:bean name="fileDownloadView" class="com.framework.innodale.controller.FileDownloadView" p:contentType="text/html" />

<!--    <beans:bean name="pdfPrintView" class="com.framework.innodale.controller.PdfPrintView" p:contentType="text/html" />-->

</beans>